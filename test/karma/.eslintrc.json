{
  "extends": [
    "eslint:recommended",
    "plugin:jasmine/recommended"
  ],
  "plugins": [
    "jasmine"
  ],
  "env": {
    "browser": false,
    "jasmine": true
  },
  "globals": {
    "spyOn": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "rules": {
    "semi": 2,
    "eol-last": 2,
    "jasmine/no-unsafe-spy": 0,
    "jasmine/new-line-before-expect": 0,
    "jasmine/prefer-toHaveBeenCalledWith": 0,
    "jasmine/new-line-between-declarations": 0
  }
}
